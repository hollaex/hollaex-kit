	"scripts": {
		"undomigrate": "sequelize db:migrate:undo:all",
		"migrate": "sequelize db:migrate",
		"start": "node app.js",
		"test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive --timeout 5000",
		"test-ci": "NODE_ENV=test node ./test/testRunner.js",
		"dev:plugin": "docker exec -it server_hollaex-kit-server_1 node plugins/dev.js"
	},
	"nyc": {
		"exclude": [
			"db",
			"config",
			"test",
			"app.js",
			"constants.js"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "eslint --quiet --fix"
	}
